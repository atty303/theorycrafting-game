    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.0.7",
    "http-server": "^14.1.0",
    "stmux": "^1.8.3",
    "vite": "^2.7.2"
  }
}
