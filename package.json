{
  "name": "theorycrafting-game",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "stmux -w always -e ERROR -m beep,system -- [ \"npm run dev:mill\" .. \"npm run dev:vite\" ]",
